{
  "name": "euler-oracles-scraper",
  "version": "2.0.0",
  "description": "Scraper for Euler Finance oracle data - TypeScript + Bun",
  "type": "module",
  "scripts": {
    "scrape": "bun run src/1_scrape-euler-adapters/scrape-euler-oracles.ts",
    "analyze-vendors-isolated": "bun run src/2_analyze-adapters-isolated/analyze-vendors.ts",
    "analyze-cross-adapters": "bun run src/2_analyze-cross-adapters/analyze-cross-oracles.ts",
    "analyze-vaults": "bun run src/3_analyze-euler-vaults/index.ts",
    "analyze-vaults:1-routers": "bun run src/3_analyze-euler-vaults/steps/1-fetch-routers.ts",
    "analyze-vaults:2-vaults": "bun run src/3_analyze-euler-vaults/steps/2-fetch-vaults.ts",
    "analyze-vaults:3-balances": "bun run src/3_analyze-euler-vaults/steps/3-fetch-balances.ts",
    "analyze-vaults:4-aggregate": "bun run src/3_analyze-euler-vaults/steps/4-aggregate-vendors.ts",
    "analyze-router-governors": "bun run src/4_analyze-router-governors/analyze-router-governors.ts",
    "rank-governors": "bun run src/5_rank-router-governors-by-tvl/rank-governors-by-tvl.ts",
    "filter-vaults": "bun run src/4_filter-vault-analysis/filter-vault-analysis.ts"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "ethers": "^6.9.0",
    "puppeteer": "^21.6.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "bun-types": "^1.0.0",
    "typescript": "^5.3.0"
  }
}
