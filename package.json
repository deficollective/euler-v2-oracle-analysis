{
  "name": "euler-oracles-scraper",
  "version": "2.0.0",
  "description": "Scraper for Euler Finance oracle data - TypeScript + Bun",
  "type": "module",
  "scripts": {
    "scrape": "bun run src/scrape-euler-oracles.ts",
    "analyze": "bun run src/analyze-vendors.ts",
    "analyze-cross": "bun run src/analyze-cross-oracles.ts",
    "analyze-vaults": "bun run src/analyze-vault-vendors.ts",
    "analyze-governors": "bun run src/analyze-vault-governors.ts",
    "rank-governors": "bun run src/rank-governors-by-tvl.ts",
    "filter-vaults": "bun run src/filter-vault-analysis.ts"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "ethers": "^6.9.0",
    "puppeteer": "^21.6.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "bun-types": "^1.0.0",
    "typescript": "^5.3.0"
  }
}
